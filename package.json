{
  "name": "foam-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@vx/event": "^0.0.143",
    "@vx/gradient": "^0.0.143",
    "@vx/grid": "^0.0.146",
    "@vx/group": "^0.0.143",
    "@vx/mock-data": "^0.0.144",
    "@vx/point": "^0.0.143",
    "@vx/scale": "^0.0.143",
    "@vx/shape": "^0.0.146",
    "@vx/tooltip": "^0.0.143",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "d3-request": "^1.0.6",
    "deck.gl": "^4.1.2",
    "geolib": "^2.0.24",
    "latlon-geohash": "^1.1.0",
    "lodash": "^4.17.4",
    "luma.gl": "^4.0.3",
    "ngeohash": "^0.6.0",
    "node-sass": "^4.8.3",
    "rbush": "^2.0.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-map-gl": "^3.0.4",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "viewport-mercator-project": "^4.1.1"
  },
  "scripts": {
    "preinstall": "git submodule init && git submodule update && cd src/foam.css/ && git checkout master",
    "build-css": "node-sass src/foam.css/styles -o src/foam.css/dist",
    "watch-css": "npm run build-css && node-sass src/foam.css/styles -o src/foam.css/dist --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  }
}
